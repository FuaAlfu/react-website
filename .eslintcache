etails>\r\n    )}\r\n    </>\r\n  );\r\n};\r\n\r\nconst Details = styled(motion.div)`\r\ncolor: white;\r\n`;\r\n\r\nconst HeadLine = styled.div`\r\nmin-height: 90vh;\r\npadding-top: 20vh;\r\nposition: relative;\r\nh2{\r\n  position: absolute;\r\n  top: 10%;\r\n  left: 50%;\r\n  transform: translate(-50%, -10%);\r\n}\r\nimg{\r\n  width: 100%;\r\n  height: 70vh;\r\n  object-fit: cover;\r\n}\r\n`;\r\n\r\nconst Awards = styled.div`\r\nmin-height: 80vh;\r\ndisplay: flex;\r\nmargin: 5rem 10rem;\r\nalign-items: center;\r\njustify-content: space-around;\r\n`;\r\n\r\nconst AwardStyle = styled.div`\r\npadding: 5rem;\r\nh3{\r\n  font-size: 2rem;\r\n}\r\n.line{\r\n  width: 80%;\r\n  background: #23d997;\r\n  hieght: 0.5rem;\r\n  margin: 1rem 0rem;\r\n}\r\np{\r\n  padding: 2rem 0rem;\r\n}\r\n`;\r\n\r\nconst ImageDisplay = styled.div`\r\nmin-height: 50vh;\r\nimg{\r\n  width: 100%;\r\n  height: 100vh;\r\n  object-fit: cover;\r\n}\r\n`;\r\n\r\n//award component\r\nconst Award = ({title,description}) =>{\r\n  return(\r\n    <AwardStyle>\r\n    <h3>{title}</h3>\r\n    <div className=\"line\"></div>\r\n    <p>{description}</p>\r\n    </AwardStyle>\r\n  )\r\n}\r\n\r\nexport default MovieDetail\r\n","D:\\ProjectsToBuildUp\\projects\\000-myProjects\\000-practice\\study\\007-capture-project\\src\\movieState.js",[],"D:\\ProjectsToBuildUp\\projects\\000-myProjects\\000-practice\\study\\007-capture-project\\src\\animation.js",[],"D:\\ProjectsToBuildUp\\projects\\000-myProjects\\000-practice\\study\\007-capture-project\\src\\components\\Wave.js",[],"D:\\ProjectsToBuildUp\\projects\\000-myProjects\\000-practice\\study\\007-capture-project\\src\\components\\Toggle.js",[],"D:\\ProjectsToBuildUp\\projects\\000-myProjects\\000-practice\\study\\007-capture-project\\src\\components\\useScroll.js",["88"],"D:\\ProjectsToBuildUp\\projects\\000-myProjects\\000-practice\\study\\007-capture-project\\src\\components\\UseScroll.js",["89"],{"ruleId":"90","replacedBy":"91"},{"ruleId":"92","replacedBy":"93"},{